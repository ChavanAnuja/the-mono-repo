<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
	
 <changeSet author="Anuja Chavan" id="20240529">		
		<createTable tableName="new_patient" schemaName="patient_service_db">
			<column name="patientId" type="int">
				<constraints primaryKey="true" />
			</column>
			<column name="patientNameEnglish" type="varchar(200)">
				<constraints nullable="false" />
			</column>
			<column name="patientNameMarathi" type="varchar(200)">
				<constraints nullable="false" />
			</column>
			<column name="mobileNumber" type="varchar(200)">
				<constraints nullable="false" />
			</column>
			<column name="gender" type="varchar(200)">
				<constraints nullable="false" />
			</column>
			<column name="address" type="varchar(200)">
				<constraints nullable="false" />
			</column>
			
			<column name="birthDate" type="date">
				<constraints nullable="false" />
			</column>
			<column name="firstExaminationDate" type="date">
				<constraints nullable="false" />
			</column>
			
		</createTable>
		
	</changeSet>
	
	
	
</databaseChangeLog>
